bool checkArithmeticAverage(double average, double frequency) {
  return average >= 6 && frequency >= 0.75;
}

String checkArithmeticAverage1(double average, double frequency) {
  if(checkArithmeticAverage(average, frequency)) {
    return "aprovado";
  } else {
    return "reprovado";
  }
}

