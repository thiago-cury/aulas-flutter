bool checkArithmeticAverage(double average, double frequency) {
  return average >= 6 && frequency >= 0.75;
}

String checkArithmeticAverage1(double average, double frequen88cy) {
  if(checkArithmeticAverage(average, frequency)) {
    return "aprovado";
  } else {
    return "reprovado";
  }
}

String checkSituation(double average) {
  if(average >= 9) {
    return "A";
  } else if(average >= 8 ) {
    return "B";
  } else if(average >= 6) {
    return "C";
  }
//   return "D";
}

String checkMonth(int month) {
  switch(month) {
    case 1: return "Janeiro";
    case 2: return "Fevereiro";
    case 3: return "Março";
    case 4: return "Abril";
    default: return "Mês desconhecido";
  }
}

